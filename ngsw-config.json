{
	"$schema": "./node_modules/@angular/service-worker/config/schema.json",
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
			}
		}
	],
	"dataGroups": [
		{
			"name": "champion-api",
			"urls": ["https://cdn.merakianalytics.com/**"],
			"cacheConfig": {
				"maxSize": 100,
				"maxAge": "1d",
				"timeout": "10s",
				"strategy": "performance"
			}
		},
		{
			"name": "champion-images",
			"urls": [
				"https://ddragon.leagueoflegends.com/**",
				"https://*.leagueoflegends.com/**/*.png",
				"https://*.leagueoflegends.com/**/*.jpg"
			],
			"cacheConfig": {
				"maxSize": 200,
				"maxAge": "7d",
				"strategy": "performance"
			}
		}
	]
}
